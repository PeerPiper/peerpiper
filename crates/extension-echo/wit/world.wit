package component:extension@0.1.0;

interface types {
    record message {
        topic: string,
        peer: string,
        data: list<u8>
    }
}

interface handlers {
    use types.{message};
    handle-message: func(msg: message) -> string;
}

/// An example world for the component to target.
world extension-world {
    export handlers;
}
